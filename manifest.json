{
  "manifest_version": 3,
  "name": "Google Form Autofill",
  "version": "1.0",
  "description": "Automatically fills in Google Forms with first options",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://docs.google.com/forms/*"
  ]
}
